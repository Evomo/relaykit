# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'iOS Framework' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOS Framework

  target 'iOS Framework Unit Tests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick', :git => "https://github.com/Quick/Quick.git", :branch => "master"
    pod 'Nimble', :git => "https://github.com/Quick/Nimble.git", :branch => "master"
  end

end

target 'watchOS Framework' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for watchOS Framework

end
